# Environment Configuration for UniHousingNG
# Copy this file to .env and fill in your actual values

# Google Maps API Keys
GOOGLE_MAPS_API_KEY_ANDROID=your_android_api_key_here
GOOGLE_MAPS_API_KEY_IOS=your_ios_api_key_here
GOOGLE_MAPS_API_KEY_WEB=your_web_api_key_here

# Firebase Configuration (when you add Firebase later)
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_APP_ID=your_firebase_app_id

# Backend API Configuration
API_BASE_URL=https://api.unihousingng.com
API_VERSION=v1
API_TIMEOUT=30000

# App Configuration
APP_NAME=UniHousingNG
APP_VERSION=1.0.0
DEBUG_MODE=true

# Third-party Services
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_ID=your_analytics_id_here

# Payment Configuration (for future use)
PAYSTACK_PUBLIC_KEY=your_paystack_public_key
FLUTTERWAVE_PUBLIC_KEY=your_flutterwave_public_key

# Social Media Links
FACEBOOK_URL=https://facebook.com/unihousingng
TWITTER_URL=https://twitter.com/unihousingng
INSTAGRAM_URL=https://instagram.com/unihousingng
WEBSITE_URL=https://unihousingng.com

# Support Configuration
SUPPORT_EMAIL=support@unihousingng.com
SUPPORT_PHONE=+234XXXXXXXXXX
